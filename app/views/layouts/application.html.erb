<!DOCTYPE html>
<html>
<head>
  <title>Outgame</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link rel="icon" type="image/png" href="<%= image_path('favicon.png') %>"/>
  <link href='http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800' rel='stylesheet' type='text/css'>
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js"></script>
</head>
<body>
<nav>
  <a class="tab" href="mockup-login"><img src="<%= image_path('exit.svg') %>"/></a>
  <div class="divider"></div>
  <a class="tab" href=<%= new_event_path%>><img src="<%= image_path('add.svg') %>"/></a>
  <div class="divider"></div>
  <a class="tab" href=<%= events_path%>><img src="<%= image_path('list.svg') %>"/></a>
  <div class="divider"></div>
  <% if current_user %>
    <span> Welcome, <a class="link" href="<%= events_path%>"> <%= current_user.name %> </a></span>
    <% end %>
</nav>
<%= yield %>
</body>
</html>
